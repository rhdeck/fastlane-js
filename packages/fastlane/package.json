{
  "name": "fastlanejs",
  "version": "2.149.5",
  "description": "typed interface to Fastlane",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clear": "rm cached_api.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node setversion.js &&  yarn -s generator > src/index.ts && tsc && chmod +x dist/bin.js",
    "clearbuild": "yarn clear && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhdeck/fastlane-js.git"
  },
  "keywords": [
    "fastlane"
  ],
  "author": "Ray Deck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhdeck/fastlane-js/issues"
  },
  "homepage": "https://github.com/rhdeck/fastlane-js#readme",
  "devDependencies": {
    "@fastlanejs/api": "^1.1.4",
    "@fastlanejs/generator": "^3.0.6",
    "@raydeck/livelink": "^1.3.4",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "concat-md": "^0.3.5",
    "docts": "^0.2.0",
    "eslint": "^7.2.0",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^6.0.1",
    "rimraf": "^3.0.2",
    "showdown": "^1.9.1",
    "typedoc": "^0.17.7",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "^3.9.5"
  },
  "liveLink": {
    "liveLinks": {
      "@fastlanejs/generator": "../make-fastlane",
      "@fastlanejs/base": "../fastlane-base"
    },
    "ignoreMasks": [
      "src/index.ts",
      "dist"
    ]
  },
  "dependencies": {
    "@fastlanejs/base": "^2.0.7",
    "commander": "^5.1.0",
    "ts-node": "^8.10.2"
  },
  "bin": "./dist/bin.js",
  "files": [
    "dist/*",
    "src/*"
  ]
}
